<% layout('layouts/boilerplate') %>
    <a href="/">Back to Home</a>
    <h1>All Recipes</h1>
    <a href="/recipes/new">Create a new recipe</a>
    <ul>
        <% recipes.forEach(recipe => { %>
            <li><a href="/recipes/<%=recipe._id %>"><%= recipe.title %></a></li>
        <% }) %>
    </ul>